rc:prénom rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf owl:topDataProperty .


###  http://purl.org/rcnum/onto/rc#qualite
rc:qualite rdf:type owl:DatatypeProperty ;
           owl:equivalentProperty rc:qualité ;
           rdfs:comment """Sert à repreésenter des qualités/attributs de personnes qui apparaissent dans l'index CH2. 

\"de Saint-Julien, 1544\"
\"dom, 1539\",
 \"monsr, 1539\",
 \"seigneur de Laconnex, 1536\",
 
Certains qualités ont déjà été analyés pour produire des triplets

    :qualite \"(†), 1539\",
        \"père de Maurice, 1539\",       \"père de Thivent, 1539\" .

   -->

   :pere-de :P5186,  :P5188 ;""" .


###  http://purl.org/rcnum/onto/rc#qualité
rc:qualité rdf:type owl:DatatypeProperty .


###  http://purl.org/rcnum/onto/rc#rc_toponyme
rc:rc_toponyme rdf:type owl:DatatypeProperty .


###  http://purl.org/rcnum/onto/rc#seqno
rc:seqno rdf:type owl:DatatypeProperty ,
                  owl:FunctionalProperty ;
         rdfs:comment "No. d'ordre de cet objet dans la séance où il apparait" .


###  http://purl.org/rcnum/onto/rc#texte_str
rc:texte_str rdf:type owl:DatatypeProperty .


###  http://purl.org/rcnum/onto/rc#titre_str
rc:titre_str rdf:type owl:DatatypeProperty .


###  http://purl.org/rcnum/onto/rc#variante
rc:variante rdf:type owl:DatatypeProperty .


###  http://purl.org/rcnum/onto/rc#version
rc:version rdf:type owl:DatatypeProperty ;
           rdfs:comment """- v1 - texte brut
- v2 - normalisation Registres EdG
- v3b - normalisation RCnum 
- v4 - modernisation""" .


###  http://purl.org/rcnum/onto/rc#voir-aussi
rc:voir-aussi rdf:type owl:DatatypeProperty ;
              rdfs:comment "Renvoie à un autre patronyme" .


#################################################################
#    Classes
#################################################################

###  http://purl.org/rcnum/onto/rc#Acteur
rc:Acteur rdf:type owl:Class ;
          owl:equivalentClass cidoc-crm:E39_Actor ;
          rdfs:subClassOf rc:Entité_RC .


###  http://purl.org/rcnum/onto/rc#Action
rc:Action rdf:type owl:Class ;
          owl:equivalentClass cidoc-crm:E7_Activity ;
          rdfs:subClassOf rc:Entité_RC .


###  http://purl.org/rcnum/onto/rc#Bien_Matériel
rc:Bien_Matériel rdf:type owl:Class ;
                 rdfs:subClassOf rc:Entité_RC ,
                                 cidoc-crm:E72_Legal_Object .


###  http://purl.org/rcnum/onto/rc#Bâtiment
rc:Bâtiment rdf:type owl:Class ;
            rdfs:subClassOf rc:Objet_immobilier .


###  http://purl.org/rcnum/onto/rc#Bâtiment_Administratif
rc:Bâtiment_Administratif rdf:type owl:Class ;
                          rdfs:subClassOf rc:Bâtiment .


###  http://purl.org/rcnum/onto/rc#Condamnation
rc:Condamnation rdf:type owl:Class ;
                rdfs:subClassOf rc:Action ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty rc:coupable ;
                                  owl:someValuesFrom rc:Acteur
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty rc:infraction ;
                                  owl:someValuesFrom rc:Infraction
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty rc:peine ;
                                  owl:someValuesFrom rc:Peine
                                ] .


###  http://purl.org/rcnum/onto/rc#Crime
rc:Crime rdf:type owl:Class ;
         rdfs:subClassOf rc:Infraction .


###  http://purl.org/rcnum/onto/rc#Division_Administrative
rc:Division_Administrative rdf:type owl:Class ;
                           rdfs:subClassOf rc:Entité_Géographique .


###  http://purl.org/rcnum/onto/rc#Document
rc:Document rdf:type owl:Class ;
            rdfs:subClassOf rc:Entité_RC ,
                            cidoc-crm:E31_Document .


###  http://purl.org/rcnum/onto/rc#Enfants_de
rc:Enfants_de rdf:type owl:Class ;
              rdfs:subClassOf rc:Ensemble_de_Personnes .


###  http://purl.org/rcnum/onto/rc#Ensemble_de_Personnes
rc:Ensemble_de_Personnes rdf:type owl:Class ;
                         rdfs:subClassOf rc:Entité_RC ,
                                         cidoc-crm:E74_Group .


###  http://purl.org/rcnum/onto/rc#Entité_Géographique
rc:Entité_Géographique rdf:type owl:Class ;
                       rdfs:subClassOf rc:Entité_RC ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty rc:code_postal ;
                                         owl:allValuesFrom xsd:string
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty rc:nom_actuel ;
                                         owl:allValuesFrom xsd:string
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty rc:nom_arrondissement ;
                                         owl:allValuesFrom xsd:string
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty rc:nom_canton ;
                                         owl:allValuesFrom xsd:string
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty rc:nom_chef_lieu ;
                                         owl:allValuesFrom xsd:string
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty rc:nom_département ;
                                         owl:allValuesFrom xsd:string
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty rc:nom_pays ;
                                         owl:allValuesFrom xsd:string
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty rc:nom_région ;
                                         owl:allValuesFrom xsd:string
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty rc:rc_toponyme ;
                                         owl:allValuesFrom xsd:string
                                       ] .


###  http://purl.org/rcnum/onto/rc#Entité_RC
rc:Entité_RC rdf:type owl:Class .


###  http://purl.org/rcnum/onto/rc#Famille
rc:Famille rdf:type owl:Class ;
           rdfs:subClassOf rc:Ensemble_de_Personnes .


###  http://purl.org/rcnum/onto/rc#Folio
rc:Folio rdf:type owl:Class ;
         rdfs:subClassOf rc:Objet_Physique ,
                         cidoc-crm:E22_Human-Made_Object ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty rc:numero ;
                           owl:someValuesFrom xsd:string
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty rc:image ;
                           owl:allValuesFrom xsd:anyURI
                         ] .


###  http://purl.org/rcnum/onto/rc#Infraction
rc:Infraction rdf:type owl:Class ;
              rdfs:subClassOf rc:Action ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty rc:auteur ;
                                owl:someValuesFrom rc:Acteur
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty rc:auteur ;
                                owl:allValuesFrom rc:Acteur
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty rc:victime ;
                                owl:allValuesFrom rc:Acteur
                              ] .


###  http://purl.org/rcnum/onto/rc#Lieu
rc:Lieu rdf:type owl:Class ;
        owl:equivalentClass cidoc-crm:E53_Place ;
        rdfs:subClassOf rc:Entité_Géographique .


###  http://purl.org/rcnum/onto/rc#Lieu_Habité
rc:Lieu_Habité rdf:type owl:Class ;
               rdfs:subClassOf rc:Lieu ;
               rdfs:comment "(geonames) a city, town, village, or other agglomeration of buildings where people live and work" .


###  http://purl.org/rcnum/onto/rc#Objet
rc:Objet rdf:type owl:Class ;
         rdfs:subClassOf rc:Document ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty rc:texte ;
                           owl:someValuesFrom rc:Texte
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty rc:décision ;
                           owl:allValuesFrom [ rdf:type owl:Class ;
                                               owl:unionOf ( rc:Condamnation
                                                             rc:Payement
                                                           )
                                             ]
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty rc:lemme ;
                           owl:allValuesFrom rc:Texte
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty rc:folio_debut ;
                           owl:someValuesFrom rdfs:Literal
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty rc:seqno ;
                           owl:someValuesFrom xsd:integer
                         ] ;
         rdfs:comment """Cette classe comprend la partie d'un pv de séances relative à un objet précis.

La propriété seqno indique le numéro d'ordre de cet objet dans le PV de la séance""" .


###  http://purl.org/rcnum/onto/rc#Objet_Physique
rc:Objet_Physique rdf:type owl:Class ;
                  rdfs:subClassOf rc:Entité_RC ,
                                  cidoc-crm:E22_Human-Made_Object .


###  http://purl.org/rcnum/onto/rc#Objet_immobilier
rc:Objet_immobilier rdf:type owl:Class ;
                    rdfs:subClassOf rc:Bien_Matériel .


###  http://purl.org/rcnum/onto/rc#Objet_mobilier
rc:Objet_mobilier rdf:type owl:Class ;
                  rdfs:subClassOf rc:Bien_Matériel .


###  http://purl.org/rcnum/onto/rc#Office
rc:Office rdf:type owl:Class ;
          rdfs:subClassOf rc:Role .


###  http://purl.org/rcnum/onto/rc#Participation
rc:Participation rdf:type owl:Class ;
                 rdfs:subClassOf rc:Document ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty rc:colonne ;
                                   owl:someValuesFrom xsd:integer
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty rc:ligne ;
                                   owl:someValuesFrom xsd:integer
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty rc:nom_participant ;
                                   owl:someValuesFrom xsd:string
                                 ] .


###  http://purl.org/rcnum/onto/rc#Partie
rc:Partie rdf:type owl:Class ;
          rdfs:subClassOf rc:Document ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty rc:titre ;
                            owl:someValuesFrom rc:Texte
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty rc:objet ;
                            owl:allValuesFrom rc:Objet
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty rc:position_participant ;
                            owl:allValuesFrom rc:Participation
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty rc:folio_debut ;
                            owl:someValuesFrom xsd:string
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty rc:seqno ;
                            owl:someValuesFrom xsd:integer
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty rc:nom_participant ;
